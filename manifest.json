{
  "manifest_version": 3,
  "name": "LabRat - Research Assistant",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "storage",
    "sidePanel"
  ],
  "side_panel": {
    "default_path": "sidepanel.html"
  },
  "content_scripts": [{
    "matches": ["*://*.snowflakecomputing.com/*"],
    "js": ["content.js"]
  }],
  "action": {
    "default_title": "Open LabRat Drawing Panel"
  }
}